<h1><%= @user.name %></h1>
<h2><%= @user.email %></h2>
<%= "My Reservations" %><br/>
<% @reservations.each do |reservation| %>
<p>
<%= link_to reservation.restaurant.name, restaurant_path(reservation.restaurant) %>
<%= reservation.time.strftime('%I:00 %p') %>
Guests #<%= reservation.guests%></p><br>
<% end %>

<a href="/restaurants/new">Create New Restaurant</a>


